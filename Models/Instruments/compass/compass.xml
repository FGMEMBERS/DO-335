<?xml version="1.0"?>
<PropertyList>

 <path>compass.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Compass</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.5</red>
    <green>0.25</green>
    <blue>0.25</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Compass</object-name>
  <property>/orientation/heading-deg</property>
  <axis>
    <x>1</x>
    <y>0</y>
    <z>0</z>
  </axis>
 </animation>

</PropertyList>
