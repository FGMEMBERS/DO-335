<?xml version="1.0"?>

<PropertyList>

 <path>do335.ac</path>
 <offsets>
   <x-m>  0.000 </x-m>
   <y-m>  0.000 </y-m>
   <z-m>  0.000 </z-m>
 </offsets>
 
 <animation>
   <object-name>disqueA</object-name>
   <object-name>disqueB</object-name>
   <object-name>heliceA</object-name>
   <object-name>heliceB</object-name>
   <object-name>bolA</object-name>
   <object-name>bolB</object-name>
   <object-name>aileronA</object-name>
   <object-name>aileronG</object-name>
   <object-name>gourverne</object-name>
   <object-name>profondeur</object-name>
   <object-name>flapA</object-name>
   <object-name>flapG</object-name>
   <object-name>roueA</object-name>
   <object-name>roueG</object-name>
   <object-name>roueD</object-name>
   <object-name>axeA1</object-name>
   <object-name>axeG1</object-name>
   <object-name>axeD1</object-name>
   <object-name>porteG</object-name>
   <object-name>porteD</object-name>
 </animation>

<!-- Les hélices --> 
 <animation>
   <type>select</type>
   <object-name>disqueA</object-name>
   <condition>
     <greater-than>
       <property>/engines/engine[0]/rpm</property>
       <value>1000</value>
     </greater-than>
   </condition>
 </animation>

 <animation>
   <type>select</type>
   <object-name>disqueB</object-name>
   <condition>
     <greater-than>
       <property>/engines/engine[1]/rpm</property>
       <value>1000</value>
     </greater-than>
   </condition>
 </animation>

  <animation>
    <type>select</type>
    <object-name>heliceA</object-name>
    <condition>
      <less-than>
        <property>/engines/engine[0]/rpm</property>
	<value>1500</value>
      </less-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>heliceB</object-name>
    <condition>
      <less-than>
        <property>/engines/engine[1]/rpm</property>
	<value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>disqueA</object-name>
    <object-name>heliceA</object-name>
    <object-name>bolA</object-name>
    <property>/engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -6.484 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.039 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>disqueB</object-name>
    <object-name>heliceB</object-name>
    <object-name>bolB</object-name>
    <property>/engines/engine[1]/rpm</property>
    <factor> -1 </factor>
    <center>
      <x-m> 6.209 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.447 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

<!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>gouverne</object-name>
    <property>/controls/flight/rudder</property>
    <factor>20</factor>
    <offset-deg> 0 </offset-deg>
    <axis>
      <x1-m>  5.434 </x1-m>
      <y1-m>  0.000 </y1-m>
      <z1-m> -0.910 </z1-m>
      <x2-m>  5.411 </x2-m>
      <y2-m>  0.000 </y2-m>
      <z2-m>  1.781 </z2-m>
    </axis>
  </animation>

<!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>profondeur</object-name>
    <property>/controls/flight/elevator</property>
    <factor> 10.0 </factor>
    <offset-deg> 0 </offset-deg>
    <axis>
      <x1-m>  5.275 </x1-m>
      <y1-m> -2.215 </y1-m>
      <z1-m>  0.538 </z1-m>
      <x2-m>  5.275 </x2-m>
      <y2-m>  2.215 </y2-m>
      <z2-m>  0.538 </z2-m>
    </axis>
  </animation>

<!-- Ailerons -->
 <animation>
   <type>rotate</type>
   <object-name>aileronG</object-name>
   <property>/surface-positions/left-aileron-pos-norm</property>
   <factor>18.0</factor>
   <offset-deg>0</offset-deg>   
   <axis>
     <x1-m>  0.329 </x1-m>
     <y1-m> -5.439 </y1-m>
     <z1-m>  0.127 </z1-m>
     <x2-m>  0.329 </x2-m>
     <y2-m> -2.887 </y2-m>
     <z2-m> -0.111 </z2-m>
   </axis>
 </animation>

  <animation>
    <type>rotate</type>
    <object-name>aileronD</object-name>
    <property>/surface-positions/right-aileron-pos-norm</property>
    <factor>18.0</factor>
    <offset-deg>0</offset-deg>   
    <axis>
     <x1-m>  0.329 </x1-m>
     <y1-m>  2.887 </y1-m>
     <z1-m> -0.111 </z1-m>
     <x2-m>  0.329 </x2-m>
     <y2-m>  5.439 </y2-m>
     <z2-m>  0.127 </z2-m>
    </axis>
  </animation>

<!-- Volets -->
  <animation>
    <type>rotate</type>
    <object-name>flapG</object-name>
    <property>/surface-positions/flaps-pos-norm</property>
    <factor>-55.0</factor>
    <axis>
      <x1-m>  0.176 </x1-m>
      <y1-m> -0.687 </y1-m>
      <z1-m> -0.321 </z1-m>
      <x2-m>  0.176 </x2-m>
      <y2-m> -2.887 </y2-m>
      <z2-m> -0.164 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>flapD</object-name>
    <property>/surface-positions/flaps-pos-norm</property>
    <factor>-55.0</factor>
    <axis>
      <x1-m>  0.176 </x1-m>
      <y1-m>  2.887 </y1-m>
      <z1-m> -0.164 </z1-m>
      <x2-m>  0.176 </x2-m>
      <y2-m>  0.687 </y2-m>
      <z2-m> -0.321 </z2-m>
    </axis>
  </animation>

<!-- Canopy -->
  <animation>
    <type>translate</type>
    <object-name>verriere1</object-name>
    <object-name>verriere2</object-name>
    <property>instrumentation/doors/crew/position-norm</property>
    <interpolation>
      <entry>
        <ind> 0.00 </ind>
        <dep> 0.00 </dep>
      </entry>
      <entry>
        <ind> 0.20 </ind>
        <dep> 0.17 </dep>
      </entry>
      <entry>
        <ind> 1.00 </ind>
        <dep> 0.17 </dep>
      </entry>
    </interpolation>
    <axis>
      <x>  0.000 </x>
      <y>  0.000 </y>
      <z>  1.000 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>verriere1</object-name>
    <object-name>verriere2</object-name>
    <property>instrumentation/doors/crew/position-norm</property>
    <interpolation>
      <entry>
        <ind> 0.00 </ind>
        <dep> 0.00 </dep>
      </entry>
      <entry>
        <ind> 0.20 </ind>
        <dep> 0.00 </dep>
      </entry>
      <entry>
        <ind> 1.00 </ind>
        <dep> 0.78 </dep>
      </entry>
    </interpolation>
    <axis>
      <x>  1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>


