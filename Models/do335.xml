  <!-- Planches de bord -->
  <!-- Avant -->
  <model>
    <path>Aircraft/DO-335/Models/Panels/panel.xml</path>
    <offsets>
      <x-m> -3.248 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.477 </z-m>
    </offsets>
  </model>

  <!-- Pilote invisible depuis le cockpit -->
  <animation>
    <type>select</type>
    <object-name>pilote</object-name>
    <condition>
      <greater-than>
        <property>sim/current-view/view-number</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

  <!-- HÃ©lice avant --> 
  <animation>
    <name>HeliceCompleteA</name>
    <object-name>propdiscA</object-name>
    <object-name>propblurA</object-name>
    <object-name>heliceA</object-name>
    <object-name>bolA</object-name>
  </animation>

  <!-- SÃ©lÃ©ction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdiscA</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurA</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>600.000</value>
      </greater-than>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>heliceA</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
	<value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceCompleteA</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -6.484 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.039 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

